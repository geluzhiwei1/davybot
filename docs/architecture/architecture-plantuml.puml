    CustomBaseTool <|--.. ToolExecutor : executes
}

' ========================================
' 2.3 LLM Integration Component
' ========================================

package "Component: LLM Integration" {
    class LLMProvider {
        -model_router: ModelRouter
        -circuit_breaker: CircuitBreaker
        -rate_limiter: RateLimiter
        +chat(messages)
        +stream(messages)
    }

    class ModelRouter {
        -models: List
        -load_balancer: LoadBalancer
        +route(request)
        +add_model(model)
    }

    class CircuitBreaker {
        -state: State
        -failure_count: int
        +call()
        +reset()
    }

    class RateLimiter {
        -rate: float
        +acquire()
        +release()
    }

    LLMProvider *-- ModelRouter : uses
    ModelRouter ..> CircuitBreaker : protected by
    CircuitBreaker ..> RateLimiter : limited by
}

' ========================================
' 2.4 Communication Component
' ========================================

package "Component: Communication" {
    class WebSocketManager {
        -router: MessageRouter
        -sessions: Dict
        -event_bus: EventBus
        +connect(ws)
        +disconnect(session_id)
        +broadcast(message)
    }

    class MessageRouter {
        -handlers: Dict
        +register(message_type, handler)
        +route(message)
    }

    class SessionManager {
        -sessions: Dict
        +create_session()
        +get_session()
        +close_session()
    }

    class MessageProtocol {
        <<enumeration>>
        CONNECT
        USER_MESSAGE
        STREAM_CONTENT
        TOOL_CALL
        AGENT_COMPLETE
    }

    WebSocketManager *-- MessageRouter : routes via
    WebSocketManager *-- SessionManager : manages
    MessageRouter ..> MessageProtocol : validates
}

