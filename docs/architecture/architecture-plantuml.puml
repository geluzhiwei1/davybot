package "Component: Agent Orchestration" {
    class Agent {
        -event_bus: EventBus
        -task_engine: TaskGraphExecutionEngine
        -tool_executor: IToolExecutor
        -llm_service: ILLMService
        -mode_manager: ModeManager
        +run(task_graph)
        +stop()
        +switch_mode(mode)
